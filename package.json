{
  "name": "test-github-actions-deploy",
  "version": "0.0.1",
  "private": true,
  "description": ".github/workflows/*.yml のサンドボックス環境",
  "repository": "https://github.com/kuro-kuroite/test-github-actions-deploy.git",
  "license": "MIT",
  "author": "@kuro-kuroite",
  "main": "dist/index.js",
  "scripts": {
    "build": "rome bundle index.ts dist/",
    "ci:check:format": "rome check",
    "ci:check:type": "tsc -p . --noEmit",
    "fix": "rome check --apply",
    "postinstall": "npx patch-package && npx typesync",
    "lint": "rome check",
    "serve": "node dist/index.js",
    "start": "rome run index.ts",
    "test": "rome test"
  },
  "devDependencies": {
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "rome": "^10.0.4-beta",
    "typescript": "^4.1.3",
    "typesync": "^0.8.0"
  },
  "$schema": "https://json.schemastore.org/package"
}
